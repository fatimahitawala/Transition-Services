{
   "name": "transition-services",
   "version": "1.0.1",
   "description": "Modular Tenancy Management Backend for Sobha Smart FM. Includes MoveIn, MoveOut, Renewal, Documents, Integration, and Webhook modules with full API documentation.",
   "main": "dist/index.js",
   "prettier": "@company/prettier-config",
   "author": "Amrishkhan Sheik Abdullah",
   "license": "ISC",
   "scripts": {
      "start": "ts-node src/index.ts",
      "dev": "nodemon ./src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "clean": "rimraf dist/",
      "copy-files": "copyfiles -u 1 src/**/*.yml  dist/",
      "build": "npm run clean && tsc -p . && npm run copy-files",
      "serve": "nodemon dist/index.js",
      "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/data-source.ts",
      "migrate": "typeorm migration:run -d dist/Common/data-source.js",
      "migrate:dev": "typeorm-ts-node-commonjs migration:run -d src/Common/data-source.ts",
      "migrate:generate": "typeorm-ts-node-commonjs migration:generate src/Migration/TransitionServiceChanges -d src/Common/data-source.ts"
   },
   "devDependencies": {
      "@types/bcrypt": "5.0.0",
      "@types/compression": "1.7.5",
      "@types/cors": "2.8.13",
      "@types/crypto-js": "4.2.2",
      "@types/docusign-esign": "5.19.6",
      "@types/express": "5.0.0",
      "@types/helmet": "4.0.0",
      "@types/morgan": "1.9.4",
      "@types/node": "16.11.10",
      "@types/node-schedule": "2.1.0",
      "@types/nodemailer": "6.4.7",
      "@types/passport-jwt": "3.0.8",
      "@types/swagger-jsdoc": "6.0.1",
      "@types/swagger-ui-express": "4.1.3",
      "@types/uuid": "9.0.8",
      "copyfiles": "2.4.1",
      "nodemon": "2.0.22",
      "rimraf": "5.0.0",
      "ts-node": "10.9.2",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@types/multer": "1.4.7",
      "axios": "1.4.0",
      "bcrypt": "5.1.0",
      "compression": "1.7.5",
      "cors": "2.8.5",
      "crypto-js": "4.2.0",
      "docusign-esign": "7.0.1",
      "dotenv": "16.0.3",
      "express": "4.21.2",
      "http-status": "1.6.2",
      "joi": "17.9.2",
      "jsonwebtoken": "9.0.0",
